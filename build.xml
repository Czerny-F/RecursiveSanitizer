<?xml version='1.0' encoding='UTF-8'?>
<project name="RecursiveSanitizer" default="build">
  <autoloader autoloaderpath="vendor/autoload.php" />
  <target name="phpunit">
    <exec command="vendor/bin/phpunit" checkreturn="true" logoutput="true" passthru="true" />
    <!--phpunit pharlocation="vendor/bin/phpunit" /-->
  </target>
  <target name="build" depends="phpunit" />
</project>
