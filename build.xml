<?xml version='1.0' encoding='UTF-8'?>
<project name="RecursiveSanitizer" default="build">
  <target name="phpunit">
    <exec dir="." executable="phpunit" failonerror="true">
      <arg line="--coverage-clover build/logs/clover.xml" />
    </exec>
  </target>
  <target name="build" depends="phpunit" />
</project>
